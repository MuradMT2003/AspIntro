@using AspIntro.Models;
@{
    List<Product> list = ViewBag.Products;
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1>Products:</h1>
    <ul>
        @foreach(var item in list)
        {
            <li>@item.Name</li>
        }
    </ul>
</body>
</html>
